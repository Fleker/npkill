{
  "compilerOptions": {
    "target": "es2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "sourceMap": false,
    "declaration": false,
    "rootDir": "./src",
    "outDir": "./lib/",
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@core/*": [
        "src/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ]
    }
  },
  "include": [
    "src"
  ],
}